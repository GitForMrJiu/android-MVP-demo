<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!--定义所有Theme的基本属性-->
    <style name="AppCommonTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!--<item name="android:textSize">@dimen/themetextsize</item>-->
        <!--默认主题色-->
        <item name="themeColor">@color/md_red_500</item>
        <item name="themeStatusbarColor">@color/md_red_700</item>
        <!--ActionBar颜色-->
        <item name="colorPrimary">?attr/themeColor</item>
        <!--StatusBar颜色-->
        <item name="colorPrimaryDark">?attr/themeStatusbarColor</item>
        <item name="colorAccent">?attr/themeColor</item>

        <item name="theme_background">@color/material_grey_100</item>

        <!--默认不适配，只有在19的时候在适配-->
        <!--<item name="fitSystemWindows">false</item>-->
        <item name="menuHeaderHeight">165dip</item>
        <item name="menuHeaderPaddingTop">20dip</item>
        <!--timeline-->
        <item name="text_title">@color/text_100</item>
        <item name="text_subhead">@color/text_54</item>
        <item name="text_body">@color/text_80</item>
        <item name="android:windowIsTranslucent">true</item>

        <item name="qmui_content_spacing_horizontal">@dimen/qmui_content_spacing_horizontal</item>
        <item name="android:textAppearance">@style/TextAppearance.Text.Regular</item>
        <!--**********************************************
       *                   qmui btn                     *
       ***********************************************-->
        <item name="qmui_round_btn_text_size">@dimen/qmui_btn_text_size</item>
        <item name="qmui_round_btn_border_width">@dimen/qmui_btn_border_width</item>
        <item name="qmui_round_btn_bg_color">@color/qmui_btn_blue_bg</item>
        <item name="qmui_round_btn_border_color">@color/qmui_btn_blue_border</item>
        <item name="qmui_round_btn_text_color">@color/qmui_btn_blue_text</item>
        <!--**********************************************
    *               qmui loading view                *
    ***********************************************-->
        <item name="qmui_loading_size">20dp</item>
        <item name="qmui_loading_color">@color/qmui_config_color_gray_5</item>

        <item name="QMUILoadingStyle">@style/AppCommonTheme.Loading</item>

        <!-- tip -->
        <item name="qmui_tip_dialog_bg">@drawable/qmui_tip_dialog_bg</item>
        <item name="qmui_tip_dialog_min_width">120dp</item>
        <item name="qmui_tip_dialog_min_height">56dp</item>
        <item name="qmui_tip_dialog_margin_horizontal">?attr/qmui_content_spacing_horizontal</item>
        <item name="qmui_tip_dialog_padding_vertical">12dp</item>
        <item name="qmui_tip_dialog_padding_horizontal">?attr/qmui_content_spacing_horizontal</item>
        <!--<item name="android:windowAnimationStyle">@style/default_animation</item>-->
        <item name="android:textViewStyle">@style/TextAppearance.Text.Regular</item>
    </style>


    <!-- 默认Activity跳转动画 -->
    <style name="default_animation" mce_bogus="1" parent="@android:style/Animation.Translucent">
        <item name="android:activityOpenEnterAnimation">@anim/h_fragment_enter</item>
        <item name="android:activityOpenExitAnimation">@anim/h_fragment_exit</item>
        <item name="android:activityCloseEnterAnimation">@anim/h_fragment_pop_enter</item>
        <item name="android:activityCloseExitAnimation">@anim/h_fragment_pop_exit</item>
    </style>
    <style name="NoTitleFullscreen" parent="@android:style/Theme.NoTitleBar.Fullscreen">
        <item name="android:windowIsTranslucent">true</item>
    </style>

    <style name="TextAppearance.Text.Regular" parent="android:TextAppearance">
        <item name="fontPath">fonts/source-sans-pro/Miui-Regular.ttf</item>

    </style>

    <style name="TextAppearance.Text.Bold" parent="android:TextAppearance">
        <item name="fontPath">fonts/source-sans-pro/Miui-Regular.ttf</item>
    </style>

    <style name="TextAppearance.Text.Light" parent="android:TextAppearance">
        <item name="fontPath">fonts/source-sans-pro/Miui-Regular.ttf</item>
    </style>

    <style name="TextAppearance.Text.Italic" parent="android:TextAppearance">
        <item name="fontPath">fonts/source-sans-pro/Miui-Regular.ttf</item>
    </style>
    <!--Values-vxx之间的差异属性-->
    <style name="AppBaseTheme" parent="AppCommonTheme">
        <item name="android:selectableItemBackground">@drawable/selector_list_holo</item>
        <item name="DHbuttonSelector">@drawable/selector_list_holo</item>

    </style>

    <style name="AppTheme" parent="AppBaseTheme"></style>

    <style name="AppTheme.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" >

        <item name="elevation">0dp</item>
        <item name="background">?attr/themeColor</item>
        <item name="android:windowContentOverlay">@null</item>
    </style>
    <style name="AppTheme.Main">
        <item name="android:windowIsTranslucent">true</item>
    </style>

    <style name="AppTheme.Search">
        <item name="android:windowBackground">@color/comm_transparent</item>
    </style>

    <style name="AppTheme.Profile"></style>

    <style name="AppTheme.Pics">
        <item name="themeColor">@color/comm_black</item>
    </style>

    <style name="AppTheme.PicsPre">
        <item name="themeColor">@color/comm_black</item>
    </style>


